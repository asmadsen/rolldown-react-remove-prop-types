{
	"name": "rolldown-react-remove-prop-types",
	"version": "0.1.0",
	"description": "Rolldown plugin to remove React PropTypes from production builds (Rollup compatible)",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./rollup": {
			"types": "./dist/rollup.d.ts",
			"import": "./dist/rollup.js",
			"require": "./dist/rollup.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "biome check .",
		"format": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui"
	},
	"keywords": [
		"rolldown",
		"rollup",
		"plugin",
		"react",
		"proptypes",
		"optimization"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/asmadsen/rolldown-react-remove-prop-types.git"
	},
	"author": "",
	"license": "MIT",
	"packageManager": "pnpm@10.18.2",
	"peerDependencies": {
		"rolldown": "^1.0.0",
		"rollup": "^3.0.0 || ^4.0.0"
	},
	"peerDependenciesMeta": {
		"rolldown": {
			"optional": true
		},
		"rollup": {
			"optional": true
		}
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.3.0",
		"acorn": "^8.15.0",
		"estree-walker": "^3.0.3",
		"magic-string": "^0.30.21"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/estree": "^1.0.8",
		"@types/node": "^22.18.13",
		"@vitest/ui": "^3.2.4",
		"acorn-jsx": "^5.3.2",
		"rolldown": "1.0.0-beta.45",
		"rollup": "^4.52.5",
		"tsdown": "^0.15.11",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	}
}
